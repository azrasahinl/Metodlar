//Kendisine parametre olarak gelen iki boyutlu String dizisi içinde 
//3 karakterli olan Stringleri ekrana yazan metodu yazınız
package newpackage;
public class algoornek19 {
    public static void UcKarakter(String str[][]){
        for (int i = 0; i <str.length; i++) {
            for (int j = 0; j <str[0].length; j++) {
                if(str[i][j].length()==3){
                    System.out.println(str[i][j]);}
            }
        }
    }public static void main(String[] args) {
        String str[][]={{"ali","veli"},{"abc","ahmet"}};
        UcKarakter(str);  
    }
    
}
