
package büthazırlık;
public class bütörnek32 {
    public static boolean asalmı(int a){
     if(a<2)
         return false;
        for (int i =2; i <a; i++) {
            if(a%i==0)
                return false;
        }
          return true;  
        }
    public static void main(String[] args) {
        boolean x=asalmı(18);
        System.out.println(x);
    }
}
